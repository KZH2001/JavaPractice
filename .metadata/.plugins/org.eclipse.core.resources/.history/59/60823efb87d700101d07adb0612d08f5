package sumCalculation;

public class Sum {

	public static void main(String[] args) {
		
		launch();
	}
	
	private static void launch() {
		showMessage("Weclome");
	}
	
	
	
	//show message
	private static void showMessage(String message) {
		System.out.println("--------------------");
		System.out.println(message);
		System.out.println("--------------------");
	}

}
